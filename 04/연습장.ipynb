   "source": [
    "def min_coins(change):\n",
    "    coins = [500, 100, 50, 10, 5, 1]\n",
    "    count = 0\n",
    "    for coin in coins:\n",
    "        count += change // coin\n",
    "        change %= coin\n",
    "    return count\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    X = int(input())\n",
    "    change = 1000 - X\n",
    "    print(min_coins(change))\n",
    "\n",
    "\n"
   ]
